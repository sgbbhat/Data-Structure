#include <iostream>
using namespace std;

void print(int *p)
{
	while(p != NULL)
	{
		cout<<p;
		p = p+1;
	}
}

int main() {
	int * p = new int();
	p = malloc(10* sizeof(int));
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
