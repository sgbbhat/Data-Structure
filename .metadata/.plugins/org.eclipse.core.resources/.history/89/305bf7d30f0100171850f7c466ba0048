#include <iostream>
#include "ListWithTail"

using namespace std;

ListWithTail::ListWithTail()
{
	hearPtr = NULL;
	tailPtr = NULL;
	currPtr = NULL;
	tempPtr = NULL;
}

void ListWithTail::pushFront(int key)
{
	node * n = new node();
	n->key = key;

	if(hearPtr == NULL)
	{
		hearPtr = n;
		tailPtr = n;
		n->next = NULL;
	}
	else
	{
		n->next = hearPtr;
		hearPtr = n;
	}
}


int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
