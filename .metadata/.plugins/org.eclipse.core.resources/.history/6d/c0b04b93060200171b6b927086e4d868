#include <iostream>
using namespace std;

void print(int *p)
{
	for(int i=0; i<5; i++)
	{
		cout<<p[i]<<endl
		p = p+1;
	}
}

int main() {
	int * p ;
	p = new int [5] {1,2,3,4,5};
	print(p);
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
