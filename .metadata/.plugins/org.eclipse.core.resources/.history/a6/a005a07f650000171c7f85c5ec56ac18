#include <iostream>
using namespace std;

struct node
{
	int data;
	node * next;
};

class LinkedList
{
private:
	node * currPtr;
	node * tempPtr;
	node * headPtr;

public:
	LinkedList();
	void addNode(int Data);
	void deleteNode(int Data);
	void print();
};

LinkedList::LinkedList()
{
	currPtr = NULL;
	tempPtr = NULL;
	headPtr = NULL;
}

int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
