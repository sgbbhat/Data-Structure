#ifndef DOUBLELINKEDLIST_H_
#define DOUBLELINKEDLIST_H_

struct node
{
	int key;
	node * next;
	node * prev;
};

class DoubleLinkedList
{
	node * headPtr;
	node * tailPtr;
	node * currPtr;
	node * tempPtr;
};


#endif /* DOUBLELINKEDLIST_H_ */
