#include <iostream>
using namespace std;

void mergesort(int * array, int * sortedarray)
{
	mergesort(array, sortedarray, 0, 10)
}

int main() {
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
